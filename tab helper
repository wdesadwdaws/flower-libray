-- Properties for Main GUI
Converted["_Flower Gui"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Converted["_Flower Gui"].Name = "Flower Gui"
Converted["_Flower Gui"].Parent = game:GetService("CoreGui")

Converted["_MAIN UI"].Name = "MAIN UI"
Converted["_MAIN UI"].Parent = Converted["_Flower Gui"]

Converted["_FRAME"].Name = "FRAME"
Converted["_FRAME"].Parent = Converted["_MAIN UI"]

Converted["_FRAME PART 1"].BackgroundColor3 = Color3.fromRGB(171, 170, 164)
