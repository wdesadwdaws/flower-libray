-- Tabs placement
for i = 1, 4 do
	Converted["_TAB "..i.." PLACEMET"].BackgroundColor3 = Color3.fromRGB(150, 152, 152)
	Converted["_TAB "..i.." PLACEMET"].BackgroundTransparency = 0.34
	Converted["_TAB "..i.." PLACEMET"].BorderColor3 = Color3.fromRGB(0, 0, 0)
	Converted["_TAB "..i.." PLACEMET"].BorderSizePixel = 0
	Converted["_TAB "..i.." PLACEMET"].Parent = Converted["_TABS"]
end

-- Load TabHelper
loadstring(game:HttpGet("https://raw.githubusercontent.com/wdesadwdaws/flower-libray/refs/heads/main/tab%20helper",true))()

-- Now you have TabHelper with TextLabels (_Tab1, _Tab2, _tab3, _Tab4)
